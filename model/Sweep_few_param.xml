<?xml version='1.0' encoding='UTF-8'?>
<MorpheusModel version="4">
    <Description>
        <Details></Details>
        <Title></Title>
    </Description>
    <Space>
        <Lattice class="square">
            <Neighborhood>
                <Order>1</Order>
            </Neighborhood>
            <Size value="5, 6,   0.0" symbol="size"/>
        </Lattice>
        <SpaceSymbol symbol="space"/>
    </Space>
    <Time>
        <StartTime value="0"/>
        <StopTime value="100"/>
        <TimeSymbol symbol="time"/>
    </Time>
    <Analysis>
        <ModelGraph format="dot" reduced="false" include-tags="#untagged"/>
        <Gnuplotter time-step="100" name="V_n">
            <Plot title="V_n">
                <Cells value="V_n">
                    <ColorMap>
                        <Color color="gray" value="0"/>
                        <Color color="blue" value="1"/>
                    </ColorMap>
                </Cells>
            </Plot>
            <Terminal name="png"/>
        </Gnuplotter>
        <Logger time-step="1.0" name="V_grid">
            <Input>
                <Symbol symbol-ref="V"/>
                <Symbol symbol-ref="space.x"/>
                <Symbol symbol-ref="space.y"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger time-step="1.0" name="cell_prop">
            <Input>
                <Symbol symbol-ref="cell.center.x"/>
                <Symbol symbol-ref="cell.center.y"/>
                <Symbol symbol-ref="cell.type"/>
                <Symbol symbol-ref="V"/>
                <Symbol symbol-ref="V_n"/>
                <Symbol symbol-ref="inf_by"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <ContactLogger time-step="1.0"/>
        <Logger time-step="1.0" name="populations">
            <Input>
                <Symbol symbol-ref="celltype.target.size"/>
                <Symbol symbol-ref="celltype.infected.size"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger time-step="1.0">
            <Input>
                <Symbol symbol-ref="b_c2c"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
    </Analysis>
    <Global>
        <Constant value="0.0" symbol="b_c2c"/>
        <Constant value="0.0" symbol="V"/>
        <Event>
            <Condition>time >= 10</Condition>
            <Rule symbol-ref="start">
                <Expression>1</Expression>
            </Rule>
        </Event>
        <Variable value="0" symbol="start" name=""/>
    </Global>
    <CellTypes>
        <CellType class="biological" name="target">
            <VolumeConstraint strength="1" target="3"/>
            <SurfaceConstraint mode="aspherity" strength="1" target="1"/>
            <ConnectivityConstraint/>
            <Property value="0" symbol="V"/>
            <Property value="0" symbol="V_n"/>
            <NeighborhoodReporter>
                <Input value="V" scaling="length" noflux-cell-medium="true"/>
                <Output symbol-ref="V_n" mapping="average"/>
            </NeighborhoodReporter>
            <Function symbol="p_c2c">
                <Expression>1-exp(-b_c2c*V_n*start)</Expression>
            </Function>
            <ChangeCellType newCellType="infected" time-step="1" name="cell2cell infection">
                <Condition>rand_uni(0,1) &lt;= p_c2c</Condition>
                <Triggers>
                    <Rule symbol-ref="V">
                        <Expression>1</Expression>
                    </Rule>
                    <Rule symbol-ref="inf_by">
                        <Expression>1</Expression>
                    </Rule>
                </Triggers>
            </ChangeCellType>
            <Property value="0.0" symbol="inf_by"/>
            <FreezeMotion>
                <Condition>start == 1</Condition>
            </FreezeMotion>
        </CellType>
        <CellType class="biological" name="infected">
            <VolumeConstraint strength="1" target="3"/>
            <SurfaceConstraint mode="aspherity" strength="1" target="1"/>
            <ConnectivityConstraint/>
            <Property value="1" symbol="V"/>
            <Property value="0" symbol="V_n"/>
            <NeighborhoodReporter>
                <Input value="V" scaling="length" noflux-cell-medium="true"/>
                <Output symbol-ref="V_n" mapping="average"/>
            </NeighborhoodReporter>
            <Property value="0.0" symbol="inf_by"/>
            <FreezeMotion>
                <Condition>start == 1</Condition>
            </FreezeMotion>
        </CellType>
    </CellTypes>
    <CPM>
        <Interaction/>
        <ShapeSurface scaling="norm">
            <Neighborhood>
                <Order>6</Order>
            </Neighborhood>
        </ShapeSurface>
        <MonteCarloSampler stepper="edgelist">
            <MCSDuration value="1"/>
            <MetropolisKinetics temperature="1"/>
            <Neighborhood>
                <Order>2</Order>
            </Neighborhood>
        </MonteCarloSampler>
    </CPM>
    <CellPopulations>
        <Population type="target" size="99" name="target">
            <InitDistribute number-of-cells="9" probability="1" mode="random"/>
        </Population>
        <Population type="infected" size="1" name="infected">
            <InitDistribute number-of-cells="1" probability="1" mode="random"/>
        </Population>
    </CellPopulations>
</MorpheusModel>
